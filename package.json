{
  "name": "horizon-final",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "local-ssl-proxy --source 3000 --target 3001 --cert ~/.mkcert/localhost.pem --key ~/.mkcert/localhost-key.pem",
    "build": "next build",
    "start": "next start",
    "prepare": "husky",
    "format": "bunx @biomejs/biome format . --write && bunx @biomejs/biome lint . --write",
    "lint": "bunx @biomejs/biome check .",
    "lint:fix": "bunx @biomejs/biome format . --write && bunx @biomejs/biome lint . --write",
    "lint:unsafe": "bunx @biomejs/biome check . --write --unsafe",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.2",
    "@stomp/stompjs": "^7.2.1",
    "@tanstack/react-query": "^5.90.5",
    "dayjs": "1.11.19",
    "js-cookie": "^3.0.5",
    "local-ssl-proxy": "^2.0.5",
    "next": "15.4.6",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.65.0",
    "react-loading-skeleton": "^3.5.0",
    "react-spinners": "^0.17.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "@chromatic-com/storybook": "^4.1.1",
