    "@storybook/addon-backgrounds": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-links": "^9.1.13",
    "@storybook/addon-measure": "^9.0.8",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/addon-outline": "^9.0.8",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/addon-vitest": "^9.1.13",
    "@storybook/blocks": "^8.6.14",
    "@storybook/nextjs-vite": "^9.1.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/browser": "^4.0.3",
    "@vitest/coverage-v8": "^4.0.1",
    "husky": "^9.1.7",
    "playwright": "^1.56.1",
    "storybook": "^9.1.13",
